{
    "name": "amritgc/ramro-framework",
    "description": "Ramro is a progressive php framework.",
    "keywords": ["framework", "micro-framework", "progressive-framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Amrit G.C",
            "email": "music.demand01@gmail.com"
        }
    ],
    "require": {
        "league/container": "^2.4",
        "league/route": "^3.0",
        "zendframework/zend-diactoros": "^1.4"
    },
    "provide": {
      "psr/container-implementation": "1.0",
      "psr/http-message-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Ramro\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
